    "cypress": "^5.6.0",
    "express": "^4.17.1",
    "history": "^5.0.0",
    "lodash": "^4.17.20",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-loading-skeleton": "^2.1.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "reduxsauce": "^1.2.0",
    "reselect": "^4.0.0",
    "styled-components": "^5.2.1",
    "styled-theming": "^2.2.0",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4",
    "@types/react-redux": "^7.1.11",
    "@types/lodash": "^4.14.165",
    "@types/react-router-dom": "^5.1.6",
    "@types/redux-logger": "^3.0.8",
    "@types/redux-mock-store": "^1.0.2",
    "@types/styled-components": "^5.1.4",
    "@types/styled-theming": "^2.2.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "coverage": "react-scripts test --collectCoverage && open coverage/index.html",
    "eject": "react-scripts eject",
    "format": "prettier --list-different 'src/**/*.{js,jsx,ts,tsx,json,htm,html}'",
    "format:fix": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,htm,html}'",
    "lint": "eslint src/ --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src/ --ext .ts,.tsx,.js,.jsx --fix",
    "test:e2e:dev": "cypress open"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/*.{json,js,jsx,ts,tsx}": [
      "npm run format:fix",
      "npm run lint:fix"
    ]
  },
  "devDependencies": { 
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "redux-mock-store": "^1.5.4"
  },
  "homepage": "https://github-search-az.herokuapp.com/"
}
